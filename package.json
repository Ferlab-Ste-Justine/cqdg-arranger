{
  "name": "cqdg-arranger",
  "version": "1.0.0",
  "description": "GraphQL API",
  "main": "src/index.js",
  "scripts": {
    "start": "(cd ./docker; ./launch.sh); npm run start-nodocker",
    "start-nodocker": "cross-env NODE_ENV=development nodemon -w .env -w src --exec babel-node ./src/index.js",
    "stop": "(cd ./docker; ./teardown.sh);",
    "build": "babel ./src -d dist",
    "prod": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ferlab-Ste-Justine/cqdg-arranger.git"
  },
  "author": "Patrice Laplante",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ferlab-Ste-Justine/cqdg-arranger/issues"
  },
  "homepage": "https://github.com/Ferlab-Ste-Justine/cqdg-arranger#readme",
  "dependencies": {
    "@arranger/server": "^2.16.1",
    "@babel/runtime": "^7.12.5",
    "@elastic/elasticsearch": "^7.9.1",
    "adm-zip": "^0.5.5",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.8.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "filesize": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "http-status-codes": "^2.1.4",
    "keycloak-connect": "11.0.3",
    "papaparse": "^5.3.1",
    "regenerator-runtime": "^0.13.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.0.5",
    "prettier-eslint": "^10.1.1"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  }
}
